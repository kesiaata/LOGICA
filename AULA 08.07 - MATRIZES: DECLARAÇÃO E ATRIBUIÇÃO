  programa
  {
    inteiro m[3][3]//Matriz quadrada
    inteiro lin, col

    funcao inicio()
    {
    //Inserindo valores na matriz
      para(lin = 0; lin <= 2; lin++){
        para(col = 0; col <= 2; col++){
          escreva("Digite um número: ")
          leia(m[lin][col])
        }
      }

      escreva("Valor na posição 1,2: "+ m[1][2]+ "\n")

    //Escrever os dados da matriz na tela:
      para(lin = 0; lin <= 2; lin++){
        para(col = 0; col <= 2; col++){
          se(col < 2){
            escreva(m[lin][col]+ " ")
          }
          senao{
            escreva(m[lin][col]+ "\n")
          }
        }
      }
    }	
  }
